                <div class="form-group">
                    <input type="" inputmode="numeric" placeholder="latitude" value="" name="latStart" class="sea-blue">
                </div>
            </div>
            <div class="group">
                <h3 class="group-heading">End</h3>
                <div class="form-group">
                    <input type="" inputmode="numeric" placeholder="longitude" value="" name="longend" class="sea-blue">
                </div>
                <div class="form-group">
                    <input type="" inputmode="numeric" value="" placeholder="latitude" name="latEnd" class="sea-blue">
                </div>
            </div>
            <button type="submit" class="submit-button">view</button>
        </form>
      <button class="downloadButton">DownLoad</button>

    </div>
    <div>
        <table>
  <thead>
    <tr>
      <th>FID</th>
      <th>LONGITUDE</th>
      <th>LATITUDE</th>
